<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.photoprint">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CAMERA" />

    <uses-feature android:name="android.hardware.camera" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".Activities.ComplainActivity"></activity>
        <activity android:name=".Activities.WishListActivity" />
        <activity
            android:name=".Activities.Photo_Editor.CropImageActivity"
            android:label="Crop Image"
            android:theme="@style/AppTheme1" />
        <activity
            android:name=".Activities.Photo_Editor.ImagePickerActivity"
            android:label="Pick Image" />
        <activity android:name=".Activities.AddImageActivity" />
        <activity android:name=".Activities.FullImageActivity" />
        <activity android:name=".Activities.AllCaseActivity" />
        <activity android:name=".Activities.CategoryCaseActivity" />
        <activity android:name=".Activities.CaseDetailsActivity" />
        <activity android:name=".Activities.SecurityActivity" />
        <activity android:name=".Activities.AddressActivity" />
        <activity android:name=".Activities.OffersActivity" />
        <activity android:name=".Activities.MobileModelActivity" />
        <activity android:name=".Activities.MobileBrandActivity" />
        <activity android:name=".Activities.AccountActivity" />
        <activity android:name=".Activities.DetailsActivity" />
        <activity android:name=".Activities.ProductsActivity" />
        <activity android:name=".Activities.ContactActivity" />
        <activity android:name=".Activities.CartActivity" />
        <activity android:name=".Activities.OrderActivity" />
        <activity android:name=".Activities.MallActivity" />
        <activity android:name=".Activities.RegisterActivity" />
        <activity android:name=".Activities.LoginActivity" />
        <activity android:name=".Activities.HomeActivity" />
        <activity android:name=".Activities.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>